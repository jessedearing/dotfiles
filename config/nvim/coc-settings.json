{
  "languageserver": {
    "dot": {
      "command": "dot-language-server",
      "args": ["--stdio"],
      "filetypes": ["dot"]
    }
  },
  "yaml.schemas": {
    "Kubernetes": "*.yaml"
  },
  "python.pythonPath": "python3",
  "suggest.snippetIndicator": "·",
  "go.goplsPath": "/home/jesse/go/bin/gopls",
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true
  },
  "go.goplsArgs": [
    "-remote=auto"
  ],
  "diagnostic.warningSign": "⁉",
  "diagnostic.errorSign": "‼"
}
