snippet hand
	http.HandlerFunc(func(w http.ResponseWriter, r *http.Request) {
		${0}
	})
endsnippet

# define attribute when instantiating a struct
snippet dat
	${1:Name}: ${2:Value},${0}
endsnippet

# attribute with GoDoc
snippet at
	// $1 ${3}
	${1:Name} ${2:Type}${0}
endsnippet

snippet fu "func with comment"
	// $1 ${5}
	func ${4} ${1:FunctionName}(${2}) ${3} {
		${0}
	}
endsnippet
